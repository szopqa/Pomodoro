        xmlns:local="clr-namespace:Pomodoro"
        mc:Ignorable="d"
        Title="Pomodoro" Height="600" Width="807.835" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">

    <Grid Background="#FF7C7C7C">
